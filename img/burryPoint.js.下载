list.cutdown=function(begintime,endtime,index){var total_micro_second;var t;count_down(f(begintime,endtime));var leaveDay=date_format(total_micro_second).day;var leaveHr=date_format(total_micro_second).hr;var leaveMin=date_format(total_micro_second).min;if(leaveDay&&leaveDay!=0){if(leaveDay>0){$($('.ju-prodbanner-logo').find("span[name='dateNow']")[index]).html("<i class='icon-clock'></i>仅剩<em class='days'>"+leaveDay+"</em>天");}else if(leaveDay<=0){$($('.ju-prodbanner-logo').find("span[name='dateNow']")[index]).html("<i class='icon-clock'></i>已结束");}}else if(leaveHr>=1){$($('.ju-prodbanner-logo').find("span[name='dateNow']")[index]).html("<i class='icon-clock'></i>仅剩<em class='days'>"+leaveHr+"</em>小时");}else if(leaveHr<1){$($('.ju-prodbanner-logo').find("span[name='dateNow']")[index]).html("<i class='icon-clock'></i>仅剩<em class='days'>"+leaveMin+"</em>分");}
function f(da1,da2){var dd1=da1.substring(0,10)+'T'+da1.substring(11,19);var dd2=da2.substring(0,10)+'T'+da2.substring(11,19);var d1=new Date(dd1);var d2=new Date(dd2);var d3=d2-d1;total_micro_second=d3;return total_micro_second}
function count_down(total_micro_second){if(total_micro_second<=0){$($('.ju-prodbanner-logo').find("span[name='dateNow']")[index]).html("<i class='icon-clock'></i>已结束");return;}
var t;if(t){clearTimeout(t)}
t=setTimeout(function(){total_micro_second-=1000;count_down(total_micro_second);},1000)}
function date_format(micro_second){var second=Math.floor(micro_second/1000);var hr=Math.floor(second/3600)<10?Math.floor(second/3600):Math.floor(second/3600);var day=Math.floor(hr/24)<10?Math.floor(hr/24):Math.floor(hr/24);var min=fill_zero_prefix(Math.floor((second-hr*3600)/60));var sec=fill_zero_prefix((second-hr*3600-min*60));var micro_sec=fill_zero_prefix(Math.floor((micro_second%1000)/10));var secHr=(hr-day*24)<10?(hr-day*24):(hr-day*24);return{hr:hr>24?secHr:hr,day:day,min:min,sec:sec,cuttime:"天"+hr+"时"+min+"分"+sec+"秒",day:day,cuttime1:day+"天"+secHr+"时"+min+"分"+sec+"秒"}}
function fill_zero_prefix(num){return num<10?"0"+num:num}}
