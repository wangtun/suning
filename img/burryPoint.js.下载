function formatDateTime(timeStamp){var date=new Date();date.setTime(timeStamp);var y=date.getFullYear();var m=date.getMonth()+1;m=m<10?('0'+m):m;var d=date.getDate();d=d<10?('0'+d):d;var h=date.getHours();h=h<10?('0'+h):h;var minute=date.getMinutes();var second=date.getSeconds();minute=minute<10?('0'+minute):minute;second=second<10?('0'+second):second;return y+'-'+m+'-'+d+' '+h+':'+minute+':'+second;};list.maidian=function(){$('[name]').on('click',function(){try{sa.click.sendDatasIndex(this);}catch(e){}});function runAnalyseExpo(){if(typeof _analyseExpoTags=="function"){_analyseExpoTags("a");}else{setTimeout(runAnalyseExpo,1000);}}
