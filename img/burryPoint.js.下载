var gps=ids.join(',');var callBack='gbstatus'+flagFn+flag;var url='/proxy/nmps/ajax/batchCommsStatusByDjh_'+gps+'_'+callBack+'.html';$.ajax({url:url,type:'get',dataType:'jsonp',jsonpCallback:callBack,success:function(res){if(res.code=='0'){deferred.resolve(res);}},error:function(){deferred.reject()}});return deferred.promise();};function getSuningPrice(ids,flagFn,flag,wrap){var deferred=$.Deferred();if(ids.length<=0){return false;}
var partNumbers=[],vendorCodes=[];for(var i=0;i<ids.length;i++){var item=ids[i].split('_');if(item[1].length<18){partNumbers.push('000000000000000000'.substring(0,18-item[1].length)+item[1]);}else{partNumbers.push(item[1]);}
if(item[2]=='0'){vendorCodes.push('0000000000');}else{vendorCodes.push(item[2]);}}
var cityCode=ju.util.getCookie('SN_CITY')&&ju.util.getCookie('SN_CITY').split('_')[1]||'025',area='',chan='1',sys='nmps',callBack='snstatus'+flagFn+flag;var url=JU_ENV.DOMAIN.ICPS_HOST[ENV]+'/icps-web/queryMpsPrices/'+partNumbers.join(',')+'_'+cityCode+'_'+area+'_'+chan+'_'+vendorCodes.join(',')+'_'+sys+'_'+callBack+'.vhtm';$.ajax({url:url,type:'get',dataType:'jsonp',jsonpCallback:callBack,success:function(data){deferred.resolve(data);},error:function(){deferred.reject()}});return deferred.promise();};ju.getPriceAndStatus=function(ids_gb,ids_sn,flagFn,flag,wrap){$.when(getMultiSalePrice(ids_gb,flagFn,flag,wrap),getSuningPrice(ids_sn,flagFn,flag,wrap)).done(function(djhRes,icpsRes){var djhData=djhRes.commStatusList,icpsData=icpsRes,bookArr=[];for(var i=0;i<djhData.length;i++){var param=param||{};param.grppurId=djhData[i].grppurId;param.obj=wrap.find("[data-grppurId='"+param.grppurId+"']");param.partNumber=ju.util.formatPartNumber(djhData[i].partnumber);param.vendorCode=ju.util.formatVendorCode(djhData[i].vendorCode);param.gbPrice=djhData[i].gbPrice||param.obj.attr('gbPrice');param.salesNum=djhData[i].salesNum;param.disSalesNum=djhData[i].disSalesNum;param.gbstatus=djhData[i].status;param.isSetmeal=djhData[i].isSetmeal;param.saleStatus=djhData[i].saleStatus;param.vendorName=djhData[i].vendorName;param.snPrice=icpsData[i].snPrice;param.refPrice=icpsData[i].refPrice;param.mpsPrice=icpsData[i].mpsPrice||param.obj.attr('gbPrice');param.snStatus=icpsData[i].status;param.subCode=icpsData[i].subCode;param.status=param.gbstatus||param.snStatus;param.defSubComm=ju.util.formatPartNumber(param.obj.attr('data-defSubComm'));param.enctyptId=param.obj.attr('data-enctyptId');param.dataSrc=param.obj.attr('dataSrc');param.type=param.obj.attr('data-type');param.obj.attr('data-status',param.status)
param.obj.attr('data-gbPrice',param.mpsPrice)
param.obj.attr('data-snPrice',param.snPrice)
param.obj.attr('data-salenum',param.salesNum)
if(param.type=='isBrand'){param.collectId=param.obj.attr('data-collectId');ju.util.formatBrandProdLink(param);}else{ju.util.formatCommonProdLink(param);}
if(param.isSetmeal=='11'){var b_partNumber=param.defSubComm;bookArr.push(param.vendorCode+'_'+b_partNumber);}else{ju.util.renderPrice(param);ju.util.renderStatus(param);ju.util.renderSaleNum(param);}
param.obj.removeAttr('rel');}
if(bookArr.length>=1){ju.getBookData(bookArr,wrap);}}).fail(function(){ju.makeUpPrice($('.ju-prodlist-item'));if($('.ju-bc-bktj').length>0){ju.makeUpPrice($('.ju-bc-bktj'));}});}
ju.makeUpPrice=function($obj){$obj.each(function(index,item){var $item=$(item);var $text=$item.find('.sale-price');var gbprice=$item.attr('gbprice');gbprice&&$text.html('<i>&yen;</i>'+ju.util.formatPrice(gbprice));});}
ju.getBookData=function(ids,wrap){var callBack='book'+new Date().getTime();var url=JU_ENV.DOMAIN.YUSHOU_HOST[ENV]+'/book/outerIntf/getBatchActionInfoForNMPS-'+ids+'-'+callBack+'.htm';$.ajax({url:url,type:'get',dataType:'jsonp',jsonpCallback:callBack,success:function(res){if(res.code=='0'){var dataList=res.data;for(var i=0;i<dataList.length;i++){var param=param||{};param.partNumber=dataList[i].partNumber.replace(/^0+/,'');param.obj=wrap.find("[data-defSubComm='"+param.partNumber+"']");param.bookPrice=dataList[i].bookPrice;param.depositAmount=dataList[i].depositAmount;param.bookedNum=dataList[i].bookedNum;param.isBooked=dataList[i].isBooked;ju.util.renderBookPrice(param);ju.util.renderBookStatus(param);ju.util.renderStatus(param);ju.util.renderBookNum(param);}}}})}
ju.util={getTimestamp:function(){return new Date().getTime();},getJSONP:function(url,callback){return $.ajax({url:url,dataType:'jsonp',jsonpCallback:callback});},getParam:function(strName){var strHref=window.location.href;var intPos=strHref.indexOf('?');var strRight=strHref.substr(intPos+1);var arrTmp=strRight.split('&');for(var i=0;i<arrTmp.length;i++){var arrTemp=arrTmp[i].split('=');if(arrTemp[0].toUpperCase()==strName.toUpperCase())return arrTemp[1];}
return'';},unique5:function(array){var r=[];for(var i=0,l=array.length;i<l;i++){for(var j=i+1;j<l;j++)
if(array[i]==array[j])j=++i;r.push(array[i]);}
return r;},getCookie:function(key){var arr,reg=new RegExp("(^| )"+key+"=([^;]*)(;|$)");if(arr=document.cookie.match(reg))
return unescape(arr[2]);else
return null;},formatPartNumber:function(partNumber){return partNumber=partNumber&&partNumber.length<18?('000000000000000000'.substring(0,18-partNumber.length)+partNumber):partNumber;},formatVendorCode:function(vendorCode){return vendorCode=vendorCode=='0'?'0000000000':vendorCode;},formatBrandLink:function(collectId){return JU_ENV.DOMAIN.JU_HOST[ENV]+'/pc/brandComm-'+collectId+'-1.html';},formatCommonProdLink:function(param){var final_partNumber=param.subCode||param.defSubComm||param.partNumber;var final_vendorCode=param.vendorCode;var url='';if(ju.commoditFourPageSwitch==1){url=JU_ENV.DOMAIN.PRODUCT_HOST[ENV]+'/'+final_vendorCode+'/'+final_partNumber+'.html';}else{url=JU_ENV.DOMAIN.JU_HOST[ENV]+'/pc/jusp/product-'+param.enctyptId+'.html';}
param.obj.find('.prod-link').attr('href',url);},formatBrandProdLink:function(param){var final_partNumber=param.subCode||param.defSubComm||param.partNumber;var final_vendorCode=param.vendorCode;var url='';if(ju.brandFourPageSwitch==1){url=JU_ENV.DOMAIN.JU_HOST[ENV]+'/pc/brandComm-'+param.collectId+'-1.html';}else{if(ju.commoditFourPageSwitch==1){url=JU_ENV.DOMAIN.PRODUCT_HOST[ENV]+'/'+final_vendorCode+'/'+final_partNumber+'.html';}else{url=JU_ENV.DOMAIN.JU_HOST[ENV]+'/pc/jusp/product-'+param.enctyptId+'.html';}}
param.obj.find('.prod-link').attr('href',url);},renderPrice:function(param,flagFn){var result,gb_price=Number(param.mpsPrice),sn_price=Number(param.snPrice),ref_price=Number(param.refPrice),$obj=param.obj;if(gb_price==0){$obj.find('.sale-price').html('');}else{$obj.find('.sale-price').html('<i>&yen;</i>'+ju.util.formatPrice(param.mpsPrice));}
if(!param.snPrice){result='';}
if(gb_price<sn_price){if(param.refPrice){if(gb_price<ref_price){result=ju.util.formatPrice(param.refPrice)}else if(gb_price==ref_price){result=''}else if(gb_price>ref_price){result=''}}else{result=ju.util.formatPrice(param.snPrice)}}
else if(gb_price==sn_price){if(param.refPrice){if(gb_price<ref_price){result=ju.util.formatPrice(param.refPrice)}else if(gb_price==ref_price){result=''}else if(gb_price>ref_price){result=''}}else{result=''}}
else if(gb_price>sn_price){result=''}
if(result!=''){$obj.find('.sn-price').html('&yen'+result);}else{$obj.find('.sn-price').html('');}},renderStatus:function(param){var $obj=param.obj;var status=param.status||$obj.attr('data-status');var gbPrice=param.gbPrice||$obj.attr('data-gbPrice');var snPrice=param.snPrice||$obj.attr('data-snPrice');var _hasClass=$obj.hasClass('ju-soldout-item');var pageFlag=$obj.attr('data-pageFlag');if(pageFlag==1){return}
if(!status||!Number(status)){if(!_hasClass){$obj.addClass('ju-soldout-item');$obj.find('.prod-link').append('<div class="mask"></div><i class="icon-soldout"></i>')}}else{switch(Number(status)){case-1:if(!_hasClass){$obj.addClass('ju-soldout-item');$obj.find('.prod-link').append('<div class="mask"></div><i class="icon-finished"></i>');}
break;case 2:if(!_hasClass){$obj.addClass('ju-soldout-item');$obj.find('.prod-link').append('<div class="mask"></div><i class="icon-soldout"></i>');}
break;case 3:if(!_hasClass){$obj.addClass('ju-soldout-item');$obj.find('.prod-link').append('<div class="mask"></div><i class="icon-soldout-buy"></i>');}
break;case 1:break;}}},renderSaleNum:function(param){var $obj=param.obj;var status=param.status;var salesNum=param.salesNum;var disSalesNum=param.disSalesNum;if(param.obj.attr('data-pageFlag')==1){if(disSalesNum==0){if(Number(status)!==1){$obj.find('.sale-amount').html('赶快围观吧');return;}else{$obj.find('.sale-amount').html('赶快围观吧');}}else{if(disSalesNum>99999){var strSales=disSalesNum+"";var salesTemp=strSales.substring(0,strSales.length-4);$obj.find('.sale-amount').html('<i class="icon-fire"></i><span>'+salesTemp+'万+</span>人想买');}else{if(disSalesNum>=100){$obj.find('.sale-amount').html('<i class="icon-fire"></i><span>'+disSalesNum+'</span>人想买');}else{$obj.find('.sale-amount').html('<span>'+disSalesNum+'</span>人想买');}}}}else{if(salesNum==0){if(Number(status)!==1){$obj.find('.sale-amount').html('');return;}else{$obj.find('.sale-amount').html('赶快去抢吧');}}else{if(salesNum>99999){var strSales=salesNum+"";var salesTemp=strSales.substring(0,strSales.length-4);$obj.find('.sale-amount').html('<i class="icon-fire"></i><span>'+salesTemp+'万+</span>件已售');}else{if(salesNum>=1000){$obj.find('.sale-amount').html('<i class="icon-fire"></i><span>'+salesNum+'</span>件已售');}else{$obj.find('.sale-amount').html('<span>'+salesNum+'</span>件已售');}}}}},renderBookPrice:function(param){var $obj=param.obj;var bookPrice=param.bookPrice||$obj.attr('gbPrice');var depositAmount=param.depositAmount;$obj.find('.sale-price').html('<i>&yen;</i>'+ju.util.formatPrice(bookPrice));if(depositAmount!=''){$obj.find('.deposit-price').html('定金：&yen;'+ju.util.formatPrice(depositAmount));}},renderBookStatus:function(param){var $obj=param.obj;var isBooked=param.isBooked;var _hasClass=$obj.hasClass('ju-soldout-item');if(isBooked==true){if(!_hasClass){$obj.addClass('ju-soldout-item');$obj.find('.prod-link').append('<div class="mask"></div><i class="icon-soldout"></i>')}}},renderBookNum:function(param){var $obj=param.obj;var status=$obj.attr('data-status');var bookedNum=param.bookedNum;if(bookedNum==0){if(Number(status)!==1){return;}else{$obj.find('.sale-amount').html('赶快去抢吧');}}else{if(bookedNum>99999){var strSales=bookedNum+"";var salesTemp=strSales.substring(0,strSales.length-4);$obj.find('.sale-amount').html('<i class="icon-fire"></i><span>'+salesTemp+'万+</span>件已定');}else{if(bookedNum>=1000){$obj.find('.sale-amount').html('<i class="icon-fire"></i><span>'+bookedNum+'</span>件已定');}else{$obj.find('.sale-amount').html('<span>'+bookedNum+'</span>件已定');}}}},formatPrice:function(price){var rePrice;if(!price){rePrice=price;}else{var priceArr=price.split('.');if(priceArr.length==2){var p=priceArr[1];var flag=true;for(var i=0;i<p.length;i++){if(p.charAt(p.length-1)==0){priceArr[1]=priceArr[1].substring(0,priceArr[1].length-1);}
if(p.charAt(i)!=0){flag=false;break;}}
if(flag){rePrice='<em>'+priceArr[0]+'</em>';}else{rePrice='<em>'+priceArr[0]+'<b>.'+priceArr[1]+'</b></em>';}}else{rePrice=price;}}
return rePrice;}}
